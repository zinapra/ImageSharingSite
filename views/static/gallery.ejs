<div class="item active">
	    <div class="container col-sm-12">
        <div id="slide1" class="masonry">
            <% Object.keys(files).forEach(function (key) { %> 
            <div>
                <a href="?q=<%= files[key].userName%>">                      
                <img class="col-lg-2 col-md-3 col-sm-4 col-xs-6 grid-item" 
                nothing="img-responsive img-thumbnail" 
                style="margin-bottom:20px; ax-height:250px" 
                src="images/<%= files[key].path %>">
            </div>
            <% }) %>
        </div>
    </div>
</div>